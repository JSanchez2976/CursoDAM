package ej09;

public class Tabla {
	private int numero;
	public Tabla() {
	}
	public Tabla(int numero) {
		this.numero=numero;
	}
	@Override
	public String toString() {
		return "Tabla [numero=" + numero + "]";
	}
	
	public void mostrarTablaMultiplicar() {
		for (int i = 1; i <= 10; i++) {
			System.out.println(i+" * "+numero+" = "+(i*numero));
		}
	}
}
