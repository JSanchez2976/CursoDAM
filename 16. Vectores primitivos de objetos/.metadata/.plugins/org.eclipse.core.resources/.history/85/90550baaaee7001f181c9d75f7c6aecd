package ej01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in=new Scanner(System.in);
		System.out.print("Cuantos coches vas a meter: ");
		int n=in.nextInt();
		Coche[] coches=new Coche[n];
		for (int i = 0; i < coches.length; i++) {
			coches[i].rellenar();
		}
		in.close();
	}

}
